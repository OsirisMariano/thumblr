<div id="post_content">
  <h1 clas="title">
    <%= @post.title %>
  </h1>

  <p class="date">
    Submitted <%= time_ago_in_words(@post.created_at) %> ago |
    <%= link_to  "Edit", edit_administrate_post_path(@post) %> |
    <%= link_to "Delete", administrate_post_path(@post), method: :delete, data: {confir: "Are you sure?" } %>
  </p>

  <p class="body">
  <%= @post.body %>
  </p>
</div>